{
    "name": "cubecubed",
    "version": "1.0.9",
    "description": "Play eye-catching math visualization on the web using SVG",
    "scripts": {
        "dev": "vite",
        "tsc": "tsc",
        "tsc:watch": "tsc --watch",
        "docs": "typedoc --hideInPageTOC --allReflectionsHaveOwnDocument && cp -rf ./docs/README.md ./docs/reference/",
        "build": "vite build",
        "test": "tap test/**/*.test.ts --node-arg=--require=ts-node/register"
    },
    "bin": {
        "cubecubed": "./ccw.js",
        "ccw": "./ccw.js"
    },
    "dependencies": {
        "d3": "^7.1.1",
        "inquirer": "^8.2.0",
        "katex": "^0.15.1",
        "p5": "^1.4.1",
        "tex-to-svg": "^0.2.0"
    },
    "devDependencies": {
        "@types/d3": "^7.1.0",
        "@types/katex": "^0.11.1",
        "@types/p5": "^1.4.1",
        "@types/tap": "^15.0.5",
        "tap": "^15.1.6",
        "ts-node": "^10.4.0",
        "typedoc": "^0.22.10",
        "typedoc-plugin-markdown": "^3.11.11",
        "typescript": "^4.5.4",
        "vite": "^2.7.4",
        "vite-tsconfig-paths": "^3.4.1"
    },
    "files": [
        "dist/**/*",
        "src/**/*",
        "init/",
        "ccw.js",
        "package.json",
        "README.md"
    ],
    "module": "dist/cubecubed.module.js",
    "main": "dist/cubecubed.js",
    "types": "dist/types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/imaphatduc/cubecubed.git"
    },
    "keywords": [
        "math",
        "animation",
        "svg",
        "graphics"
    ],
    "author": "imaphatduc",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/imaphatduc/cubecubed/issues"
    },
    "homepage": "https://imaphatduc.github.io/cubecubed/#/"
}
